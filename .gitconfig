[user]
	email = mpho112@gmail.com
	name = Mpho Mphego
[core]
	editor = nano 
	askpass = git-gui--askpass
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	push = origin $(git symbolic-ref --short HEAD)

[color]
	ui = auto
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[push]
	default = current

[http]
    sslVerify = false

[credential]
    helper = store

